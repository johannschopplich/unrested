{
  "name": "uncreate",
  "type": "module",
  "version": "0.9.2",
  "packageManager": "pnpm@7.12.1",
  "description": "Minimal, type-safe REST client using JS proxies",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/uncreate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/uncreate.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/uncreate/issues"
  },
  "keywords": [
    "ohmyfetch",
    "fetch",
    "api"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ohmyfetch": "^0.4.19",
    "ufo": "^0.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "h3": "^0.7.21",
    "listhen": "^0.3.4",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vitest": "^0.23.4"
  }
}
