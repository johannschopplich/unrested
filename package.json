{
  "name": "uncreate",
  "type": "module",
  "version": "0.9.3",
  "packageManager": "pnpm@8.5.1",
  "description": "Minimal, type-safe REST client using JS proxies",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/uncreate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/uncreate.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/uncreate/issues"
  },
  "keywords": [
    "ofetch",
    "fetch",
    "api"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "ofetch": "^1.0.1",
    "ufo": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "h3": "^1.6.6",
    "listhen": "^1.0.4",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.31.1"
  }
}
