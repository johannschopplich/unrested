{
  "name": "unrested",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@8.7.0",
  "description": "Minimal, type-safe REST client using JS proxies",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/unrested#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unrested.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unrested/issues"
  },
  "keywords": [
    "api",
    "fetch",
    "ofetch",
    "rest"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "ofetch": "^1.3.3",
    "ufo": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "h3": "^1.8.1",
    "listhen": "^1.4.3",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.3"
  }
}
